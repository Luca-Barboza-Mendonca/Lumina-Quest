<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>SocketSharp</id>
    <version>1.1.0</version>
    <authors>Giorgi Khutsishvili</authors>
    <owners>Giorgi Khutsishvili</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/grendizeras/SocketSharp</projectUrl>
    <description>CSharp socket wrapper library.
It abstracts working with sockets using channel interface. 
Any type of connection could be implemented behind the interface.</description>
    <releaseNotes>Fixed large data sending bug.
Added ReceiveContext which wraps payload and some stat data.</releaseNotes>
    <tags>Socket, SocketSharp, Socket wrapper</tags>
    <repository type="GitHub" url="https://github.com/grendizeras/SocketSharp" />
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Newtonsoft.Json" version="11.0.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>